set -x
docker stop ussuri-heat
docker rm ussuri-heat

docker run --name=ussuri-heat -p 9292:9292 -p 35357:35357 \
    -p 8774:8774 -p 5000:5000 -p 8004:8004 -p 9696:9696 \
    -e OSC_IP=10.79.111.11 -e AVI_IP=10.79.109.32 \
    -e HEAT_REPO='https://github.com/avinetworks/avi-heat' \
    -e HEAT_BRANCH=master -d -t -i avinetworks/ussuri-heat \
    /bin/bash -c "/root/install_scripts/startup"

docker logs -f ussuri-heat
# docker exec -it ussuri-heat bash
